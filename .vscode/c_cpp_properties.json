{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${default}",
                "${workspaceFolder}/src/Event/**",
                "${workspaceFolder}/src/http/**",
                "${workspaceFolder}/src/log/**",
                "${workspaceFolder}/src/NetWork/**",
                "${workspaceFolder}/src/Rtsp/**",
                "${workspaceFolder}/src/Thread/**",
                "${workspaceFolder}/src/Timer/**",
                "${workspaceFolder}/src/Util/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "path": [
                    "/usr/include",
                    "${workspaceFolder}/src/Event",
                    "${workspaceFolder}/src/http",
                    "${workspaceFolder}/src/log",
                    "${workspaceFolder}/src/NetWork",
                    "${workspaceFolder}/src/Rtsp",
                    "${workspaceFolder}/src/Thread",
                    "${workspaceFolder}/src/Timer",
                    "${workspaceFolder}/src/Util"
                ]
            },
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}